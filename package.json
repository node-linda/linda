{
  "name": "linda",
  "version": "2.0.0",
  "private": false,
  "description": "Linda implementation on Socket.IO",
  "engines": {
    "node": "6.x",
    "npm": "3.x"
  },
  "main": "lib/linda.js",
  "scripts": {
    "build": "babel src/ --out-dir lib/ --source-maps inline",
    "eslint": "eslint '{src,test}/*.js'",
    "mocha": "mocha 'test/*.js' --compilers js:babel-register -r babel-polyfill",
    "test": "npm run mocha && npm run eslint",
    "watch": "parallelshell 'npm run build -- --watch' 'chokidar \"{src,test}/*.js\" -c \"npm run eslint\" --initial'"
  },
  "keywords": [
    "linda",
    "socket.io"
  ],
  "author": "Sho Hashimoto <hashimoto@shokai.org>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-eslint": "^6.1.2",
    "babel-polyfill": "^6.9.1",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "chokidar-cli": "^1.2.0",
    "debug": "^2.2.0",
    "eslint": "^3.1.0",
    "eslint-plugin-if-in-test": "^0.2.0",
    "mocha": "^2.5.3",
    "parallelshell": "^2.0.0"
  },
  "dependencies": {
    "deep-equal": "^1.0.1"
  },
  "directories": {
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/node-linda/linda.git"
  },
  "bugs": {
    "url": "https://github.com/node-linda/linda/issues"
  },
  "homepage": "https://github.com/node-linda/linda"
}
